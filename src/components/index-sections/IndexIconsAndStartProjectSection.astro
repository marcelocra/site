---
import SocialLinks from "../SocialLinks.astro";
import Button from "../Button.astro";
import DownloadLatestCV from "../DownloadLatestCV.astro";
---

<div class="grid grid-cols-6 gap-3 items-center justify-between w-full">
  <div class="flex flex-row gap-3 col-span-1">
    <SocialLinks />
  </div>
  <div class="col-span-3 flex flex-row gap-3">
    <a href="/#contact"><Button outline shadow={false}>Start Project</Button></a
    >
    <DownloadLatestCV
      ><Button outline shadow={false}>Download CV</Button></DownloadLatestCV
    >
  </div>
</div>
